{
    "name": "pm-xtn-dom",
    "version": "1.0.9",
    "description": "PM extension DOM access",
    "type": "module",
    "main": "./src/index.ts",
    "files": [
        "./src",
        "./dist/es2020"
    ],
    "types": "./dist/es2020/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/es2020/index.d.ts",
            "import": {
                "types": "./dist/es2020/index.d.ts",
                "default": "./dist/es2020/index.mjs"
            },
            "module": "./dist/es2020/index.mjs",
            "default": "./dist/es2020/index.mjs"
        },
        "./*": {
            "types": "./dist/es2020/*.d.ts",
            "import": {
                "types": "./dist/es2020/*.d.ts",
                "default": "./dist/es2020/*.mjs"
            },
            "module": "./dist/es2020/*.mjs",
            "default": "./dist/es2020/*.mjs"
        }
    },
    "license": "MIT",
    "scripts": {
        "dev": "tsc -w",
        "build": "yarn rollup -c",
        "prepublish": "yarn build"
    },
    "dependencies": {
        "parse5": "^7.1.2"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.3",
        "@rollup/plugin-node-resolve": "^15.1.0",
        "@rollup/plugin-terser": "^0.4.3",
        "@rollup/plugin-typescript": "^11.1.2",
        "rollup": "^3.26.3",
        "tslib": "^2.6.0",
        "typescript": "^5.1.6"
    }
}
