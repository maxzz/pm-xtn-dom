{
    "name": "pm-xtn-dom",
    "version": "1.0.13",
    "description": "PM extension DOM access",
    "files": [
        "./src",
        "./dist/es2020"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/maxzz/pm-xtn-dom"
    },
    "type": "module",
    "exports": {
        "./package.json": "./package.json",
        "./ts": {
            "types": "./dist/es2020/index.d.ts",
            "import": {
                "types": "./dist/es2020/index.d.ts",
                "default": "./dist/es2020/index.mjs"
            },
            "default": "./dist/es2020/index.mjs"
        }
    },
    "typesVersions": {
        ">=3.1": {
            "ts": [
                "./dist/es2020/index.d.ts"
            ]
        }
    },
    "types": "./dist/es2020/index.d.ts",
    "main": "./dist/es2020/index.mjs",
    "scripts": {
        "dev": "tsc -w",
        "build": "yarn rollup -c",
        "prepublish": "yarn build"
    },
    "dependencies": {
        "parse5": "^7.1.2"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.3",
        "@rollup/plugin-node-resolve": "^15.1.0",
        "@rollup/plugin-terser": "^0.4.3",
        "@rollup/plugin-typescript": "^11.1.2",
        "rollup": "^3.26.3",
        "tslib": "^2.6.0",
        "typescript": "^5.1.6"
    }
}